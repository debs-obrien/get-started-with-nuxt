{
  "name": "learn-nuxt-app",
  "version": "1.0.0",
  "description": "Lets learn how to setup a nuxt app",
  "author": "Debbie O'Brien",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "stylelint": "stylelint '**/*.vue' '**/*.scss'",
    "analyze": "nuxt build -a"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run stylelint"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/style-resources": "^0.1.1",
    "cross-env": "^5.2.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.0.0",
    "nuxt-purgecss": "^0.2.0",
    "vue-burger-menu": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "husky": "^1.3.1",
    "node-sass": "^7.0.0",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3",
    "push-dir": "^0.4.1",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.10.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.5.1",
    "tailwindcss": "^0.7.3"
  }
}
